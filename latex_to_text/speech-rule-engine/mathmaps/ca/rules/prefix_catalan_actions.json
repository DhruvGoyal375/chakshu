    ],
    [
      "Action",
      "select",
      "[t] \"quantitat de selecció\" (pause:200)"
    ],
    [
      "Action",
      "row",
      "[t] CSFordinalPosition (grammar:gender=\"f\"); [t] \"fila\" (pause:200)"
    ],
    [
      "Action",
      "cell",
      "[n] ../..; [t] CSFordinalPosition (grammar:gender=\"f\"); [t] \"columna\" (pause:200)"
    ],
    [
      "Action",
      "cell-simple",
      "[t] CSFordinalPosition (grammar:gender=\"f\"); [t] \"columna\" (pause:200)"
