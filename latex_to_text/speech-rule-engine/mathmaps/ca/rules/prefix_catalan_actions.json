    ],
    [
      "Action",
      "rightsuper",
      "[t] \"superíndex dret\" (pause:200)"
    ],
    [
      "Action",
      "rightsuper-counted",
      "[t] CSFordinalPosition (grammar:gender=\"m\"); [t] \"superíndex dret\" (pause:200)"
